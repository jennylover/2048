{
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 0,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "name": "2048",
            "image": "140594921984.dkr.ecr.ap-southeast-1.amazonaws.com/ecr0627:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "FromECS",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "2048"
                }
            },
            "cpu": 0
        }
    ],
    "family": "task-2048",
}
